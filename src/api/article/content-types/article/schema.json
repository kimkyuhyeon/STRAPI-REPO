{
  "kind": "collectionType",
  "collectionName": "articles",
  "info": {
    "singularName": "article",
    "pluralName": "articles",
    "displayName": "메인 컨텐츠 관리",
    "description": "Create your blog content"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true,
      "unique": false
    },
    "slug": {
      "type": "uid",
      "targetField": "title",
      "required": true
    },
    "CKEditor": {
      "type": "customField",
      "options": {
        "preset": "defaultHtml"
      },
      "customField": "plugin::ckeditor5.CKEditor"
    },
    "order": {
      "type": "integer",
      "default": 0
    },
    "thumbnailContainer": {
      "displayName": "thumbnailContainer",
      "type": "component",
      "repeatable": false,
      "component": "shared.thumbnail-container",
      "required": true
    },
    "preViewText": {
      "type": "string"
    },
    "contentsType": {
      "type": "enumeration",
      "enum": [
        "editorial",
        "notice",
        "event",
        "campaign"
      ],
      "required": true
    },
    "displayDate": {
      "displayName": "displayDate",
      "type": "component",
      "repeatable": false,
      "component": "shared.display-date"
    },
    "pushContents": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::article.article",
      "inversedBy": "drawContents"
    },
    "drawContents": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::article.article",
      "mappedBy": "pushContents"
    },
    "SearchTag": {
      "type": "json"
    },
    "bgColor": {
      "type": "string",
      "regex": "^[a-zA-Z0-9]+$"
    },
    "textColor": {
      "type": "string",
      "regex": "^[a-zA-Z0-9]+$"
    },
    "contentsCompo": {
      "type": "dynamiczone",
      "components": [
        "shared.image-with-title",
        "shared.image-slider",
        "shared.slide-map",
        "shared.product-list"
      ]
    },
    "goToLink": {
      "type": "string"
    }
  }
}
